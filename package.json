{
  "name": "trmemo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "mobile": "npm run start -w trmemo-mobile",
    "backend": "npm run dev -w @trmemo/backend",
    "shared:build": "npm run build -w @trmemo/shared",
    "dev": "npm run shared:build && concurrently \"npm run mobile\" \"npm run backend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
  }
}